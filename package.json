{
  "name": "travis-github-status",
  "version": "0.0.0-development",
  "description": "Set statuses on github from travis for jest, flow, + eslint",
  "keywords": [
    "travis",
    "github",
    "jest",
    "eslint",
    "flow",
    "codeclimate",
    "snyk",
    "ci",
    "status"
  ],
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "start": "babel-node src/index.js lint flow jest codeclimate",
    "start:prod": "node dist/index.js lint flow jest codeclimate",
    "push": "git add --all && git commit -a -m 'build' && git push",
    "prepublish": "yarn run clean && yarn run build",
    "build": "babel src -d dist",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "jest",
    "format": "prettier --single-quote --parser=flow --semi=false --write '{src,__tests__}/**/*.js'",
    "lint": "eslint --fix src __tests__ __fixtures__",
    "clean": "rimraf dist && mkdir dist",
    "precommit": "lint-staged --verbose && npm test",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/faceyspacey/travis-github-status.git"
  },
  "author": "James Gillmore <james@faceyspacey.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/faceyspacey/travis-github-status/issues"
  },
  "homepage": "https://github.com/faceyspacey/travis-github-status#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.2",
    "eslint-plugin-react": "^7.0.0",
    "flow-bin": "^0.44.2",
    "husky": "^0.13.3",
    "jest": "^20.0.0",
    "lint-staged": "^3.4.0",
    "prettier": "^1.2.2",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.2"
  },
  "dependencies": {
    "codeclimate-test-reporter": "^0.4.1",
    "colors": "^1.1.2",
    "github": "^9.2.0",
    "snyk": "^1.29.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "flow-bin": "*",
    "jest": "*"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --parser=flow --semi=false --write",
      "eslint --fix",
      "git add"
    ]
  }
}
